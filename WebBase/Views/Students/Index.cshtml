@model IEnumerable<ContosoUniversity.Models.Student>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.uid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FirstMidName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.phoneNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.currentDegree)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.currentProgram)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.gpa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.numHours)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.personalStatement)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fluency)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.completedSemesters)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.linkedin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.resume)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.creationDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.modificatonDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.uid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FirstMidName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.phoneNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.currentDegree)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.currentProgram)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.gpa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.numHours)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.personalStatement)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fluency)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.completedSemesters)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.linkedin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.resume)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.creationDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.modificatonDate)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
